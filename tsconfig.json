{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": ["./src/components/*"],
      "@hooks/*": ["./src/common/hooks/*"],
      "@constants": ["./src/common/constants/index.ts"],
      "@helpers/*": ["./src/common/helpers/*"],
      "@navigation/*": ["./src/navigation/*"],
      "@res/*": ["./src/res/*"],
      "@screens/*": ["./src/screens/*"],
      "@types/*": ["./src/types/*"],
      "@store/*": ["./src/store/*"]
    }
  }
}
